{
  "name": "@sparkz-community/common-client-lib",
  "version": "0.1.15",
  "description": "Sparkz Community Common Client Library",
  "author": "Sparkz Community Development <dev@sparkz.community>",
  "license": "MIT",
  "main": "src/index.js",
  "files": [
    "src",
    "assets",
    "dist/*.{js,css}"
  ],
  "scripts": {
    "build": "npm run lint && npm run custom-icons && vue-cli-service build --report --target lib --name @sparkz-community/common-client-lib ./src/index.js",
    "lint": "eslint --fix --ext .js,.vue src",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "release:pre": "npm run build && npm version prerelease && npm publish --tag pre --access public && git push origin",
    "release:patch": "npm run build && npm version patch && npm publish --access public && git push origin",
    "release:minor": "npm run build && npm version minor && npm publish --access public && git push origin",
    "release:major": "npm run build && npm version major && npm publish --access public && git push origin",
    "custom-icons": "fantasticon src/assets/icons/svg -o src/public/icons/svg --normalize -t woff woff2 -g css",
    "custom-icons:watch": "watch 'npm run custom-icons' src/assets/icons/svg"
  },
  "dependencies": {
    "card-validator": "^8.1.1",
    "change-case": "^4.1.2",
    "gsap": "^3.7.0",
    "is-lower-case": "^2.0.2",
    "is-upper-case": "^2.0.2",
    "jquery": "^3.6.0",
    "json2mq": "^0.2.0",
    "lodash": "^4.17.21",
    "lower-case": "^1.1.4",
    "lower-case-first": "^2.0.2",
    "moment": "^2.27.0",
    "resize-observer-polyfill": "^1.5.1",
    "rrule": "^2.6.8",
    "smartcrop": "^2.0.4",
    "sponge-case": "^1.0.1",
    "ssn-validator": "^2.0.1",
    "swap-case": "^2.0.2",
    "title-case": "^3.0.3",
    "upper-case": "^1.1.3",
    "upper-case-first": "^2.0.2",
    "uuid": "^8.3.2",
    "vue-advanced-cropper": "^2.8.3",
    "vue-plaid-link2": "^0.2.2",
    "webrtc-adapter": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/eslint-parser": "^7.18.9",
    "@vue/babel-preset-app": "^4.4.6",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^7.10.0",
    "fantasticon": "^1.2.3",
    "node-sass": "^6.0.1",
    "sass-loader": "^10.2.0",
    "vue": "^3.2.37",
    "vue-template-compiler": "^2.7.7",
    "vuepress": "^1.5.2",
    "vuepress-plugin-demo-code": "^0.5.2"
  },
  "peerDependencies": {
    "@sparkz-community/form-gen-client-lib": "0.x",
    "feathers-pinia": "0.x",
    "pinia": "2.x",
    "quasar": "2.x",
    "vue": "3.x",
    "vue-router": "4.x"
  }
}
